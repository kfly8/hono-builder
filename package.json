{
  "name": "hono-builder",
  "version": "0.1.4",
  "description": "DEPRECATED - Builder pattern for Hono framework",
  "deprecated": "This package is deprecated. Hono framework alone is sufficient. See: https://github.com/kfly8/hono-builder#migration-from-hono-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kfly8/hono-builder.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "tsc --noEmit && vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check --cache \"src/**/*.{js,ts,tsx}\"",
    "format:fix": "prettier --write --cache --cache-strategy metadata \"src/**/*.{js,ts,tsx}\"",
    "remove-dist": "rm -rf ./dist",
    "build": "bun run remove-dist && bun ./build/build.ts",
    "postbuild": "publint",
    "release": "np"
  },
  "devDependencies": {
    "@hono/eslint-config": "^1.0.2",
    "@types/bun": "latest",
    "eslint": "^9.10.0",
    "np": "^10.2.0",
    "prettier": "^2.6.2",
    "publint": "^0.3.12",
    "vitest": "^3.2.2"
  },
  "peerDependencies": {
    "typescript": "^5",
    "hono": "^4.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "hono"
  ]
}
